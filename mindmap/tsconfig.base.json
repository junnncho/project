{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noPropertyAccessFromIndexSignature": false,
    "importHelpers": false,
    "target": "es2016",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@codegen": ["libs/codegen/src/index.ts"],
      "@external/module": ["libs/external/module/src/index.ts"],
      "@junblog/backend/*": ["apps/junblog/backend/*"],
      "@junblog/frontend/*": ["apps/junblog/frontend/*"],
      "@nogql/frontend": ["libs/nogql/frontend/src/index.ts"],
      "@nogql/frontend/*": ["libs/nogql/frontend/src/*"],
      "@nogql/module": ["libs/nogql/module/src/index.ts"],
      "@nogql/module/*": ["libs/nogql/module/src/*"],
      "@localjobs/backend/*": ["apps/localjobs/backend/*"],
      "@localjobs/frontend/*": ["apps/localjobs/frontend/*"],
      "@nogql/assets": ["libs/nogql/assets/src/index.ts"],
      "@nogql/contract": ["libs/nogql/contract/index.ts"],
      "@nogql/data-access": ["libs/nogql/data-access/src/index.ts"],
      "@nogql/module": ["libs/nogql/module/src/index.ts"],
      "@nogql/module/*": ["libs/nogql/module/src/*"],
      "@nogql/test-client": ["libs/nogql/test-client/src/index.ts"],
      "@nogql/test-server": ["libs/nogql/test-server/src/index.ts"],
      "@nogql/util": ["libs/nogql/util/src/index.ts"],
      "@nogql/util-client": ["libs/nogql/util-client/src/index.ts"],
      "@nogql/util-client/*": ["libs/nogql/util-client/src/*"],
      "@nogql/util-server": ["libs/nogql/util-server/src/index.ts"],
      "@social/data-access": ["libs/social/data-access/src/index.ts"],
      "@social/module": ["libs/social/module/src/index.ts"],
      "@social/ui-web": ["libs/social/ui-web/src/index.ts"],
      "@supportuber/backend/*": ["apps/supportuber/backend/*"],
      "@supportuber/frontend/*": ["apps/supportuber/frontend/*"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
